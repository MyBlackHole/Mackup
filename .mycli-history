+CREATE TABLE `jt_push_product` (
+  `id` int(11) unsigned NOT NULL AUTO_INCREMENT COMMENT 'auto_increment',
+  `user_id` int(11) unsigned NOT NULL DEFAULT '0' COMMENT '用户ID',
+  `operator_id` int(11) unsigned NOT NULL DEFAULT '0' COMMENT '操作员ID',
+  `type` tinyint(4) NOT NULL DEFAULT '1' COMMENT '产品来源类型， 1 自有  2 从亚马逊同步 3 跟卖',
+  `local_cate_id` int(11) NOT NULL DEFAULT '0' COMMENT '本地分类',
+  `parent_id` int(11) NOT NULL DEFAULT '0' COMMENT '父产品ID',
+  `sku` varchar(255) NOT NULL DEFAULT '' COMMENT '内部SKU',
+  `sku_alias` varchar(255) NOT NULL DEFAULT '' COMMENT 'SKU别名',
+  `is_complete` tinyint(1) NOT NULL DEFAULT '2' COMMENT '是否已完成 1 已完成 2 未完成(草稿)',
+  `is_primary_variant` tinyint(1) NOT NULL DEFAULT '1' COMMENT '是否主变体 1:主变体 2:子变体',
+  `create_time` timestamp NULL DEFAULT NULL COMMENT '创建时间',
+  `last_update_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '最后更新时间',
+  PRIMARY KEY (`id`),
+  UNIQUE KEY `I-sku-operator_id` (`sku`,`operator_id`)
+) ENGINE=InnoDB AUTO_INCREMENT=312 DEFAULT CHARSET=utf8mb4;
